-- tetris functions


-- if there is a piece that is
-- placed or swapped, do thing
function reset_piece ()
  cur_typ = nxt_typ
  nxt_typ = flr(rnd(7))+1
  reset_xy()
end


--


-- goes through the array to find 
-- and delete full rows
function clear ()

 for y=1,20 do

  full = true
 
  for x=1,10 do
 
   -- go through the row, and 
   -- check the contents
   if (array[y][x] == 0) then
    full = false
   end

  end

  -- go through all the rows
  -- above, ending at two
  if (full) then
   if y == 1 then
    array[1] = copy(blank_row)
   else

    array[y] = copy(blank_row)

    for _y=0,y-1 do
     array[y-_y] = copy(array[y-_y-1])
    end

    array[1] = copy(blank_row)

   end
  end
 end
end


