-- init and globals


-- deep copy function
function copy(o)
  local c
  if type(o) == 'table' then
    c = {}
    for k, v in pairs(o) 
    do
      c[k] = copy(v)
    end
  else
    c = o
  end
  return c
end


--


function clock (t)

  if ((time() % 1) % t == 0) then
    return true
  else
    return false
  end
end


