-- game initialization function
function _init()

  -- add 10 columns to the
  -- a nil spr default
  for n=1,10 do
    add(blank_row, 0)
  end 

  -- add 20 rows
  for m=1,20 do
    add(blank_array, copy(blank_row))
  end

  -- copying over blank values
  array = copy(blank_array)

end

