-- l r u d a b - 6 keys
btns1 = {0,0,0,0,0,0}
btns2 = {0,0,0,0,0,0}

function was_pressed(i)

  -- buttons are 0 index in btn()
  val = btns1[i+1] + btns2[i+1]
  psh = btn(i)

  -- check to see if the val 
  -- was 0, not pushed.
  if (val == 0) ret = true else ret = false
  if (psh == true) tmp = 1 else tmp = 0
  
  -- set the array value to btns val
  btns2[i+1] = btns1[i+1]
  btns1[i+1] = tmp

  return ret and psh
end


