-- tetris functions


-- handles right and left key presses
function rotate ()

	if (btn(1)) then 
    cur_rot += 1
	end

	if (btn(2)) then
		cur_rot -= 1
	end

  if (cur_rot > #pieces[cur_typ]) then
    cur_rot = 1
  end

  if (cur_rot < 1) then
    cur_rot = #pieces[cur_typ]
  end
end


