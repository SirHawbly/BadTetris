function draw_array (array)

  for y=1,20 do

    for x=1,10 do

      --spr(16+array[y][x],
   				--x*5,
   				--(y*6)-2)

      ispiece = false

      coord = {y+cur_row, x+cur_col}

      for i=1,4 do
        if (cur_coord[cur_rot] == coord) then -- todo
          print_piece(cur_typ, y, x)
          break
        end
      end

      if (not ispiece) then
        print_piece(array[y][x], y, x)
      end
    end
  end
end


--


function _draw ()

  get_mino_coords()

  cls()
  move_piece()
  draw_backdrop()
  draw_array(array)
  -- print_piece(7,1,1)
  clear()
end


