-- variables for the keys that
-- can be pressed in game
local b = {a = 4, b = 5,
											l = 0, r = 1,
											u = 2, d = 3}


-- the current state of the game
local state = "main menu"
local last_state = "main menu"
local selected = 1
local last_selected = 0
local chosen = false
local last_chose = 0


-- button push, not push
-- returns the value of a and 
-- not b being pressed
function btnpnp(b1,b2)

 return(btn(b1) and not btn(b2))
end


-- function to print to the 
-- screen
function middle_print(str,y,col)
	
	print(str, (128/2)-(#str*2),
	        y, col)
end


-- draw a highlighted string
function middle_print_high
             (str,y,c1,c2)

 middle_print(str, y-2, c1)
 middle_print(str,  y , c2)
 
end


-- reset the 'selected' variable
function reset_select()
 -- if the state changes for the
 -- first tiem reset selected 
 if (last_state != state and 
 						not reset) then 
  selected = 1
  reset = true
 end
end


-- src: https://www.lexaloffle.com/bbs/?tid=2951
-- a general copy function
function copy(o)
  local c
  if type(o) == 'table' then
    c = {}
    for key,val in pairs(o) do
      c[key] = copy(val)
    end
  else
    c = o
  end
  return c
end
