-- menu functions
--------------------------------


-- function for updating the
-- selected and choose vars
function getselected(menu)

 -- we dont care about changing
 -- selection or chose in 1/10s
	if (time()-last_selected < .1)
	then return 
 elseif (time()-last_chose < .1)
 then return
	end

	-- check if up and
	-- if selected is > 1
 if (btnpnp(b.u, b.d)) then
 	if (selected > 1) then
 	 selected -= 1
 	 
 	end
 
 -- else check down and if
 -- selected is < #menu
 elseif (btnpnp(b.d, b.u)) then
  if (selected < #menu) then
 	 selected += 1
 	 
 	end
 	
 end
 
 -- check if the item was chosen
 if (btnpnp(b.a, b.b)) then
  
  chosen = true
  reset = false
  last_state = state
  state = menu[selected]
  
 -- else if you hit b, go with 
 -- the last option (exit)
 elseif(btnpnp(b.b, b.a)) then
 	
 	state = menu[#menu]
 	
 end
 
 last_selected = time()
 last_chose = time()
 
end


-- function to print the menus
function menu (title, menu)

	-- highlight print the title
	-- starting at y = 20
	local y = 20
	
	for v in all(title) do
	
		middle_print_high(v,y,5,6)
		y = y + 10
		
	end
	
	-- print options at y=65
	y = 65
	
	for i=1,#menu do
	 
	 -- if its selected, highlight
	 if (selected == i) then
	  middle_print_high(menu[i],y,1,8)
	 
	 -- else regular print
	 else
	  middle_print(menu[i],y,8)
	  
	 end
	 
	 y = y + 10
	end
	
	-- update the selected item
	getselected(menu)

end


