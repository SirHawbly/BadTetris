function _init()
-- for the intro menu
 chosen = ""
	last = chosen
 selected = 1
 l1 = 0
 
 -- the intro menu's options
 mainmenu = {"play game",
             "load game",
              "options" ,
             "quit game"}

 -- the intro menu's options
 optnmenu = {"secrets",
             "volume",
             "main menu"}

end

-- draws a black border w/ a 
-- blue square around it
function draw_border()
 rectfill(0,0, 127,127, 0)
 rect    (0,0, 127,127, 1)
end


-- draws a string with a set col
function draw_string
             (str,x,y,loc)
             
 screenprint(str, 1, x,y, loc)
end


function draw_high_string
             (str,x,y,loc,c1,c2)

 screenprint(str, c1, x,y-2, loc)
 screenprint(str, c2, x,y, loc)
end


function draw_title()
 draw_high_string
     ("skele-boi",127,10,
     		"middle", 5,6)
		     
	draw_high_string
     ("in",127,20,
     		"middle", 5,6)
	
	draw_high_string
     ("skullduggery",127,30,
     		"middle", 5,6)
end


function draw_option()
 draw_high_string
     ("options",127,45,
     		"middle", 5,6)

 draw_high_string
     ("-------",127,55,
     		"middle", 5,6)
		     
end



-- go back to the commandline
function quitgame()
 stop()
end


-- function to start the game
function startgame()
end


-- function to pull data from
-- the cart or w/e to load game
function loadgame()
end


-- function to change options 
-- in the game
function optionmenu()
 menu(draw_option,optnmenu,last)
end


-- function to draw and use
-- the intro menu
function menu (title,menuopts)
 
	-- if the user hits up 
	-- (index starts at 1)
	if (btnchk(2,3) and 
	     tpass(l1,1) and
	     selected > 1) 
	then selected -= 1
						l1 = time()
			
	-- if they hit down 
	-- (an less than len)
	elseif (btnchk(3,2) and 
	         tpass(l1,1) and
	         selected < #menuopts) 
	then selected += 1
						l1 = time()
		
	-- if they select an item
	elseif (btnchk(4,5)) then 
	 chosen = menuopts[selected]
		
	-- if they have not chosen
	elseif(btnchk(5,4)) then	
	 chosen = menuopts[#selected]
	
	end
 
 -- start value for menu heights
 y = 65

 -- draw the bg for the game
 draw_border()
 
 -- draw the title of the game
 title()
 
 -- draw chosen
 -- draw_string("--"..chosen.."--", 100, 110, "middle")
 -- draw chosen
 -- draw_string(menuopts[selected], 100, 120, "middle")
 
 -- print menu options
 for v in all(menuopts) do
		
		-- if the selected one is the
		-- current option highlight it
	 if (v == menuopts[selected]) 
	 then draw_high_string
		         ("-- "..v.." --"
		           ,127,y,"middle",1,8)
 	
 	-- else draw it normally
  else 	  
   draw_string 	      
       ("-- "..v.." --"
       		,127,y,"middle")
 	 
  end
 	
 	-- increase the height
  y += 10
 end
 
 -- draw the current selection
 -- draw_string(tostr(selected),10,10,"middle")
end


function pausemenu()
 draw_border()
end


function savegame()
end


function _draw()
 	
 if (chosen != last) then
  selected = 1
 end
 	
	-- if there was not a choice 
	-- on the intro menu call it
	if (chosen == "")
	then menu(draw_title,mainmenu)
	
	-- deal with the choices
	elseif (chosen == "play game") 
	then startgame()
	
	elseif (chosen == "quit game") 
	then quitgame()
	
	elseif (chosen == "load game") 
	then	loadgame()
	
	elseif (chosen == "options") 
	then	optionmenu()
	
	end
end


function _update()
end

