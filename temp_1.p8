pico-8 cartridge // http://www.pico-8.com
version 14
__lua__
function _init()
	x = 64
	y = 48
	dx = 0
	dy = 0
end

function _draw()
	cls()
	-- draw the map
	map(0,0, 0,0, 16,16)
	
	-- draw the player
	spr(1, x, y)
end

function grounded()
	v=mget(flr(x+4)/8,flr(y)/8+1)
	return fget(v, 0)
end

function _update()
	-- x acceleration
	ddx = 0
	if (btn(0)) ddx=-.25
	if (btn(1)) ddx=.25
	
	-- apply x accel
	dx += ddx
	
	-- limit max speed
	if dx > 3 then
		dx = 1
	elseif dx < -3 then
		dx = -1
	end
	
	-- drag
	if ddx == 0 then
		dx *= 0.8
	end

	-- y velocity
	if grounded() then
		if (btnp(5)) then
			dy=-8
		else
			dy = 0
			-- fix position
			-- or else he'll be sunk into floor
			y = (flr(flr(y)/8)*8)
		end
	else
		-- gravity accel
		dy += 0.98
	end

	-- update position based on vel
	x += dx
	y += dy
end

